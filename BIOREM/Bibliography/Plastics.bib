	Keywords = {Cutinase,Cutinolytic esterase,Fusarium,Polyethylene terephthalate},
	Number = {8},
	Pages = {843--850},
	Pmid = {18449587},
	Title = {{Screening of tropical fungi producing polyethylene terephthalate- hydrolyzing enzyme for fabric modification}},
	Volume = {35},
