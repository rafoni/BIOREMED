	Doi = {10.1002/prot.24649},
	Issn = {10970134},
	Journal = {Proteins: Structure, Function and Bioinformatics},
	Keywords = {Alpha/beta-hydrolase fold,Aspergillus oryzae,Carboxylesterase,Catalytic triad,Disulfide,ESTHER database,Enzyme structure,Lipase,X-ray crystallography},
	Number = {10},
	Pages = {2857--2867},
